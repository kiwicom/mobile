{
  "collectCoverage": true,
  "collectCoverageFrom": ["app/**.js"],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__generated__/",
    "<rootDir>/app/config/",
    "<rootDir>/app/types/"
  ],
  "coverageReporters": ["lcov", "text", "text-summary"],
  "preset": "jest-expo",
  "verbose": false,
  "globals": {
    "__DEV__": false
  }
}
